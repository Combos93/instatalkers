<p id="notice"><%= notice %></p>

<h1>Rooms</h1>
<div class="online">
  <ul>
    <% @users.each do |user| %>
      <li class="pure">
        <%= user.nickname %>
      </li>
    <% end %>
  </ul>
</div>

<div class="js-online-users text-success online"></div>

<ul class="mt-4 list-unstyled">
  <%= render @rooms %>
</ul>

<div class="mt-2">
  <%= render 'rooms/form', room: @room %>
</div>
